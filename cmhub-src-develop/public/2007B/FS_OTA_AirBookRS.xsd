									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="PricingUnit" minOccurs="0" maxOccurs="20">
				<xs:annotation>
					<xs:documentation>A pricing unit for a passenger type code.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="FareComponent" maxOccurs="99">
