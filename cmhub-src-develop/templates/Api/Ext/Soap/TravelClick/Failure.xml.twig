<?xml version="1.0" encoding="UTF-8"?>
<OTA_ErrorRS Version="1.0"
                       TimeStamp="{{ timeStamp }}"
                       EchoToken="{{ echoToken }}"
                       xmlns="http://www.opentravel.org/OTA/2003/05">
    <Errors>
        <Error Type="4" Code="{{ code }}" ShortText="{{ message }}">
            {{ message }}
        </Error>
    </Errors>
</OTA_ErrorRS>
